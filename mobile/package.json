{
  "name": "mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "postinstall": "patch-package",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run --testPathPattern='\\.test\\.ts$'",
    "test:integration": "vitest run --testPathPattern='\\.integration\\.test\\.'",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src app",
    "lint:fix": "eslint src app --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"app/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" \"app/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.31",
    "expo-constants": "~18.0.13",
    "expo-device": "^8.0.10",
    "expo-font": "~14.0.10",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.22",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-ble-plx": "^3.5.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.15.1",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "tailwindcss": "^3.4.19",
    "uuid": "^13.0.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.18.0",
    "@testing-library/react-native": "^13.0.0",
    "@types/react": "~19.1.0",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^4.0.0",
    "eslint": "^9.18.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-check-file": "^3.3.1",
    "eslint-plugin-filename-rules": "^1.3.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-react-native-a11y": "^3.5.0",
    "globals": "^15.14.0",
    "husky": "^9.1.0",
    "lint-staged": "^15.3.0",
    "patch-package": "^8.0.1",
    "prettier": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.21.0",
    "vitest": "^4.0.17"
  },
  "private": true
}
